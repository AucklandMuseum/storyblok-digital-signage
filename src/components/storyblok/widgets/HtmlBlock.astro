---
import { storyblokEditable } from "@storyblok/astro";
import type { DsTemplateStoryblok } from "types/component-types-auckland-museum";
import { isDisplayable } from "utils/amUtils";

const { blok } = Astro.props;

if (!isDisplayable(blok)) return;

const html = blok.htmlCode;
---

<div {...storyblokEditable(blok)} class="w-full" set:html={html} />
