{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "layouts/*": [
        "layouts/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "styles/*": [
        "styles/*"
      ],
      "types/*": [
        "types/*"
      ],
    },
    "jsx": "preserve",
    "jsxImportSource": "react",
    "allowJs": true,
    "types": [
      "vite/client"
    ]
  }
}